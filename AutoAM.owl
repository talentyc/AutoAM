                  rdfs:domain bfo:BFO_0000015 ;
                  rdfs:range bfo:BFO_0000002 ;
                  rdfs:label "has input"@en ;
                  iof-av:adaptedFrom "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology"^^xsd:anyURI ;
                  iof-av:elucidation "y has input x iff x is an instance of Continuant and y is an instance of Process, such that the presence of x at the begining of y is a necessary condition for the start of y."@en .


