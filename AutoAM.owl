###  https://purl.industrialontologies.org/ontology/core/Core/functionOf
iof-core:functionOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf bfo:BFO_0000197 ;
                    owl:inverseOf iof-core:hasFunction ;
                    rdfs:domain bfo:BFO_0000034 ;
                    rdfs:range bfo:BFO_0000004 ;
                    rdfs:comment "A function inheres in its bearer at all times for which the function exists, however the function need not be realized at all the times that the function exists."@en ;
                    rdfs:label "function of"@en ;
                    skos:example "this catalysis function is a function of this enzyme"@en ;
                    iof-av:adaptedFrom "http://purl.obolibrary.org/obo/RO_0000079" ;
                    iof-av:elucidation "a relation between a function and an independent continuant (the bearer), in which the function specifically depends on the bearer for its existence"@en .


###  https://purl.industrialontologies.org/ontology/core/Core/hasComponentPartAtAllTimes
iof-core:hasComponentPartAtAllTimes rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf bfo:BFO_0000111 ,
                                                       iof-core:hasComponentPartAtSomeTime ;
                                    rdf:type owl:TransitiveProperty ;
                                    rdfs:domain [ owl:intersectionOf ( bfo:BFO_0000040
                                                                       [ rdf:type owl:Class ;
                                                                         owl:complementOf bfo:BFO_0000024
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                                    rdfs:range [ owl:intersectionOf ( bfo:BFO_0000040
                                                                      [ rdf:type owl:Class ;
                                                                        owl:complementOf bfo:BFO_0000024
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
                                    rdfs:label "has component part at all times" ;
                                    iof-av:elucidation """x has component part at all times y holds when x is a 'material entity' that is not a 'fiat object part' and y is a 'material entity' that is not a 'fiat object part' and x is 'has proper continuant part at all times' y

FOL: hasComponentPartAtAllTimes(x,y) → (MaterialEntity(x) ∧ ¬(FiatObjectPart(x))) ∧ (MaterialEntity(y) ∧ ¬(FiatObjectPart(y)))  ∧ hasProperContinuantPartAtAllTimes(x,y)""" .


